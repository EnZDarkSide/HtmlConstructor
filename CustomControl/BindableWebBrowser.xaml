<UserControl x:Class="HtmlConstructor.CustomControl.BindableWebBrowser"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HtmlConstructor.CustomControl"
             xmlns:webb="clr-namespace:DotNetBrowser.Wpf;assembly=DotNetBrowser.Wpf"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <webb:BrowserView HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Grid.Column="1" x:Name="browserView"/>
    </Grid>
</UserControl>
